<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ken.wms.dao.AccessRecordMapper">
    <resultMap id="accessRecord" type="com.ken.wms.domain.AccessRecordDO">

    </resultMap>

    <insert id="insertAccessRecord">
        insert into wms_access_record(USER_ID, USER_NAME, ACCESS_TYPE, ACCESS_TIME)
        values(#{userID}, #{userName}, #{accessType}, #{accessTime})
    </insert>
</mapper>